    "chromatic": "^6.11.0",
    "gh-pages": "^5.0.0",
    "jest-image-snapshot": "^5.2.0",
    "jest-puppeteer": "^6.1.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^19.1.2",
    "start-server-and-test": "^1.14.0",
    "webpack": "^5.74.0"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!axios)/"
    ]
  }
